{"ast":null,"code":"import React from 'react';\nexport default function IsolateTOC(toc) {\n  const text = toc;\n  const firstSplit = text.split(\"<!-- DON'T EDIT THIS SECTION, INSTEAD RE-RUN doctoc TO UPDATE -->\");\n  const secondSplit = firstSplit[1].split(\"<!-- END doctoc generated TOC please keep comment here to allow auto update -->\");\n  const table = secondSplit[0]; // console.log(table)\n\n  const tableByLines = table.split('\\n'); // console.log(tableByLines)\n\n  const tableByLinesNoEmpties = tableByLines.filter(line => line !== \"\" && line !== \"\\r\"); // console.log(tableByLinesNoEmpties)\n\n  const tblneAsString = `${tableByLinesNoEmpties}`; // console.log(typeof(tblneAsString));\n  // console.log(tblneAsString)\n\n  const removedSeparaters = tblneAsString.replace(/ /g, \"\").replace(/,/g, \"\").replace(/-/g, \"\").replace(/]/g, '').replace(/\\[/g, \"\").replace(/\\)/g, \"(\").replace(/\\(/g, \",\").replace(/\\`/g, \"\");\n  const removedSeparatersWithLevels = tblneAsString.replace(/,/g, \"\").replace(/]/g, '').replace(/\\)/g, \"(\").replace(/\\(/g, \",\").replace(/\\`/g, \"\"); // console.log(removedSeparatersWithLevels);\n  // console.log(removedSeparaters);\n\n  const cleanList = removedSeparaters.split(',');\n  const cleanListWithLevels = removedSeparatersWithLevels.split(',');\n  cleanList.pop();\n  cleanListWithLevels.pop(); // console.log(cleanList)\n  // console.log(cleanListWithLevels)\n\n  const topicToLink = {};\n\n  for (let i = 0; i < cleanList.length; i += 2) {\n    let topic = cleanList[i];\n    let link = cleanList[i + 1];\n    topicToLink[topic] = link;\n  }\n\n  ;\n  const topicWithLevelsToLink = {};\n\n  for (let i = 0; i < cleanListWithLevels.length; i += 2) {\n    let topic = cleanListWithLevels[i];\n    let topicWithLevels = cleanListWithLevels[i + 1];\n    topicWithLevelsToLink[topic] = topicWithLevels;\n  }\n\n  ;\n  const topicToTopicWithLevels = {};\n\n  for (let i = 0; i < Object.keys(topicToLink).length; i++) {\n    topicToTopicWithLevels[Object.keys(topicToLink)[i]] = Object.keys(topicWithLevelsToLink)[i];\n  }\n\n  console.log(`Object.keys(topicToLink) looks like: ${Object.keys(topicToLink)}`);\n  console.log(`Object.keys(topicWithLevelsToLink) looks like: ${Object.keys(topicWithLevelsToLink)}`);\n  console.log(`Object.keys(topicToTopicWithLevels) looks like: ${Object.keys(topicToTopicWithLevels)} `);\n  console.log(Object.keys(topicToLink).length);\n  return [topicToLink, topicToTopicWithLevels];\n}","map":{"version":3,"sources":["C:/Users/kyzam/Documents/GitHub/SSZ-SimpleSerialize/sszapp/components/isolateTOC.jsx"],"names":["React","IsolateTOC","toc","text","firstSplit","split","secondSplit","table","tableByLines","tableByLinesNoEmpties","filter","line","tblneAsString","removedSeparaters","replace","removedSeparatersWithLevels","cleanList","cleanListWithLevels","pop","topicToLink","i","length","topic","link","topicWithLevelsToLink","topicWithLevels","topicToTopicWithLevels","Object","keys","console","log"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,OAAlB;AAGA,eAAe,SAASC,UAAT,CAAoBC,GAApB,EAAyB;AAIpC,QAAMC,IAAI,GAAGD,GAAb;AAIA,QAAME,UAAU,GAAGD,IAAI,CAACE,KAAL,CAAW,mEAAX,CAAnB;AACA,QAAMC,WAAW,GAAGF,UAAU,CAAC,CAAD,CAAV,CAAcC,KAAd,CAAoB,iFAApB,CAApB;AACA,QAAME,KAAK,GAAGD,WAAW,CAAC,CAAD,CAAzB,CAVoC,CAWpC;;AACA,QAAME,YAAY,GAAGD,KAAK,CAACF,KAAN,CAAY,IAAZ,CAArB,CAZoC,CAapC;;AACA,QAAMI,qBAAqB,GAAGD,YAAY,CAACE,MAAb,CAAoBC,IAAI,IAAIA,IAAI,KAAK,EAAT,IAAeA,IAAI,KAAK,IAApD,CAA9B,CAdoC,CAepC;;AACA,QAAMC,aAAa,GAAI,GAAEH,qBAAsB,EAA/C,CAhBoC,CAiBpC;AACA;;AACA,QAAMI,iBAAiB,GAAGD,aAAa,CAACE,OAAd,CAAsB,IAAtB,EAA4B,EAA5B,EAAgCA,OAAhC,CAAwC,IAAxC,EAA8C,EAA9C,EAAkDA,OAAlD,CAA0D,IAA1D,EAAgE,EAAhE,EAAoEA,OAApE,CAA4E,IAA5E,EAAkF,EAAlF,EAAsFA,OAAtF,CAA8F,KAA9F,EAAqG,EAArG,EAAyGA,OAAzG,CAAiH,KAAjH,EAAwH,GAAxH,EAA6HA,OAA7H,CAAqI,KAArI,EAA4I,GAA5I,EAAiJA,OAAjJ,CAAyJ,KAAzJ,EAA+J,EAA/J,CAA1B;AACA,QAAMC,2BAA2B,GAAGH,aAAa,CAACE,OAAd,CAAsB,IAAtB,EAA4B,EAA5B,EAAgCA,OAAhC,CAAwC,IAAxC,EAA8C,EAA9C,EAAkDA,OAAlD,CAA0D,KAA1D,EAAi