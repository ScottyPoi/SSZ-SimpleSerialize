an title with className for scrollspy link\r\n\r\n    const topicToLevel = {};\r\n    Object.keys(topicToTWL).map((topic) => {\r\n        topicToLevel[topic] = topicToTWL[topic][0] === \"-\"\r\n            ? \"Scrollspy1\"\r\n            : topicToTWL[topic][2] === \"-\"\r\n            ? \"Scrollspy2\"\r\n            : topicToTWL[topic][4] === \"-\"\r\n            ? `Scrollspy3`\r\n            : 4\r\n    })\r\n\r\n    return topicToLevel\r\n\r\n}"]},"metadata":{},"sourceType":"module"}